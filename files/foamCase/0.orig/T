    {
        type            fixedValue;
        value           uniform 300.0;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    Flr001
    {
        type            fixedValue;
        value           uniform 500.0;
    }
    Roof001
    {
        type            fixedValue;
        value           uniform 500.0;
    }
    Wall001
    {
        type            fixedValue;
        value           uniform 500.0;
    }
    Wall002
    {
        type            fixedValue;
        value           uniform 500.0;
    }
    Wall003
    {
        type            fixedValue;
        value           uniform 500.0;
    }
    Wall004
    {
        type            fixedValue;
        value           uniform 500.0;
    }
    Win001
    {
        type            fixedValue;
        value           uniform 500.0;
    }
    
}
          


